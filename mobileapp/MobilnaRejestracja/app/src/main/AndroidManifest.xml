<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="projekt.mobilna.rejestacja.mobilnarejestracja" >

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Formularz rejestracyjny Activity -->
        <activity
            android:name=".FormularzRejestracyjnyActivity"
            android:label="Formularz " >
        </activity>

        <!-- Wolne terminy Activity -->
        <activity
            android:name=".WolneTerminyActivity"
            android:label="Wolne terminy" >
        </activity>

        <!-- Wolne godziny Activity -->
        <activity
            android:name=".WolneGodzinyActivity"
            android:label="Wolne godziny" >
        </activity>
    </application>

    <!--  Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk android:minSdkVersion="3" />

</manifest>
